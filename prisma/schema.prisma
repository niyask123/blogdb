datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id          Int      @id @default(autoincrement())
  heading     String
  title       String
  description String
  blogUrlLinks String
  blogPostDate DateTime
  imageUrl    String
}

model user {
  id          Int      @id @default(autoincrement())
  name     String
  
}
